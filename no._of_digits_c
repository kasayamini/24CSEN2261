#include <stdio.h>
int main() {
    int num, count = 0;
    printf("Enter a number: ");
    scanf("%d", &num);
    while (num != 0) {
        num /= 10;
        count++;
    }
    printf("The number of digits is: %d\n", count);
    return 0;
}



OUTPUT :
Enter a number: 2
The number of digits is: 1
