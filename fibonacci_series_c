#include <stdio.h>

void fibonacci(int n) {
    long long int a = 0, b = 1, next;

    printf("Fibonacci Series up to %d terms: \n", n);

    for (int i = 1; i <= n; i++) {
        if (i == 1) {
            printf("%lld ", a);
        } else if (i == 2) {
            printf("%lld ", b);
        } else {
            next = a + b;
            a = b;
            b = next;
            printf("%lld ", next);
        }
    }
    printf("\n");
}

int main() {
    int n;
    
    printf("Enter the number of terms for Fibonacci series: ");
    scanf("%d", &n);

    fibonacci(n);

    return 0;
}
